  body
    nav.navbar.sticky-top.navbar-expand-md.navbar-light(style='background-color: #F6A623')
      a.navbar-brand(href='#')
        img(src='img/logo.png', height='25', alt='')
        |  Contrata.me
      button.navbar-toggler.navbar-toggler-right(type='button', data-toggle='collapse', data-target='#contrataNav', aria-controls='contrataNav', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
      .collapse.navbar-collapse#contrataNav
        ul.navbar-nav
          li.navbar-item
            a.nav-link.active(href='indexUsuario.html') Inicio
        form.form-inline
          input.form-control.mr-sm-2(type='text', placeholder='Buscar', aria-label='Buscar', style='width: 30vw')
          button.btn.btn-outline-primary.my-2.my-sm-0(type='submit') Buscar
        ul.navbar-nav.ml-auto
          li.nav-item.dropdown
            a.nav-link.dropdown-toggle(data-toggle='dropdown', href='#', role='button', aria-haspopup='true', aria-expanded='false') Notificaciones
            .dropdown-menu.dropdown-menu-right
              a.dropdown-item(href='#') Notificación
              a.dropdown-item(href='#') Otra notificación
              a.dropdown-item(href='#') Tal vez otra
          li.navbar-item
            a.nav-link.active(href='perfilUsuario.html') Perfil
            span.sr-only (actual)
          li.navbar-item
            a.nav-link(href='../logout') Salir
    form
      .m-4
        .rounded.border.border-dark.mb-2.p-2.bg-primary
          .form-row
            .col.border.border-dark.border-left-0.border-top-0.border-bottom-0
              input.form-control.form-control-sm#pub-nombre(type='text', name='pub-nombre', placeholder='Nombre')
            .col.border.border-dark.border-left-0.border-top-0.border-bottom-0
              input.form-control.form-control-sm#pub-type(type='text', name='pub-type', placeholder='Tipo de trabajo')
            .col.border.border-dark.border-left-0.border-top-0.border-bottom-0
              input.form-control.form-control-sm#pub-pago(type='text', name='pub-pago', placeholder='Paga esperada')
            .col-5
              .progress(style='margin: 2px 0px 0px 0px')
                .progress-bar.bg-success(role='progress-bar', style='width: 80%; height: 28px', aria-valuenow='80', aria-valuemin='0', aria-valuemax='100')
